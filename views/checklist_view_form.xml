<?xml version="1.0" encoding="utf-8"?>
<openerp>
   <data>

      <record id="gsos_checklist_view_form" model="ir.ui.view">
         <field name="name">gsos.checklist.form</field>
         <field name="model">gsos.checklist</field>
         <field name="arch" type="xml">
            <form string="Checklist">
               <sheet>
                  <div class="oe_edit_only">
                     <label for="name"/>
                  </div>
                  <div class="oe_title">
                     <h1>
                        <field name="name" default_focus="1" placeholder="Name" />
                     </h1>
                  </div>
                  <group>
                     <group>
                        <field name="checklist_template_url" widget="url"/>
                        <field name="resolution_template_url" widget="url"/>
                     </group>
                  </group>
               </sheet>
            </form>
         </field>
      </record>

   </data>
</openerp>
